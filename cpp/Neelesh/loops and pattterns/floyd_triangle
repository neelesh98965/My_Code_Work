#include<iostream>
using namespace std;

int main()
{
    ios_base::sync_with_stdio(false);
   cin.tie(NULL);
    int n,k=1;
    cin>>n;
    for (int i = 1; i <= n; i++)
    {
        for (int j = 1; j <= i; j++)
        {
            cout<<k<<" ";
            k++;
        }
        cout<<"\n";
    }
    
    return 0;
}

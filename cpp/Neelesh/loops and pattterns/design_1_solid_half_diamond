#include <iostream>
using namespace std;

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    int n,k = 3;
    cin >> n;
    for (int i = 0; i < n; i++)
    {
        for (int j = 0; j <= i; j++)
        {
            cout <<k<<"";
        }
        k++;
        cout << "\n";
    }
    for (int i = 0; i < n; i++)
    {
        k--;
        for (int j = 0; j < n-i-1; j++)
        {
            cout <<k-1;
        }
        cout << "\n";
    }
    return 0;
}

#include<iostream>
using namespace std;

int main()
{   
    ios_base :: sync_with_stdio(false);
    cin.tie(NULL);
    int n,k = 1;
    cin>>n;
    for (int i = 0; i < n; i++)
    {
        for (int j = n; j > i ; j--)
        {
            cout<<k<<" ";
            k++;
        }
        k = 1;
        cout<<endl;
    }
       
    return 0;
}

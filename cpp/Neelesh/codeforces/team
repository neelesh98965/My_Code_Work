#include<bits/stdc++.h>
using namespace std;

int main(){
    int t,count1 = 0;
    cin>>t;
    while(t--){
        int arr[3],count = 0;
        for(int i = 0; i<3;i++){
            cin>>arr[i];
            if(arr[i] == 1){
                count++;
            }
        }
        if(count >= 2){
            count1++;
        }
    }
    cout<<count1;
}

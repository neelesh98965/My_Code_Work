#include<bits/stdc++.h>
using namespace std;
int main(){
    int count1 = 0;
    string s;
    cin>>s;
    for(char i = 'a';i<='z';i++){
        int count = 0;
        //  cout<<i;
        for(int j = 0; j< s.length();j++){
            if(s[j] == i){
                count++;
                // cout<<count<<"\n";
            }
        }
             if(count >= 1){
                count1++;
                // cout<<count1<<"\n";
            }
    }
    if(count1%2 == 0){
        cout<<"CHAT WITH HER!";
    }
    else{
        cout<<"IGNORE HIM!";
    }
    // cout<<count1<<"\n";
}

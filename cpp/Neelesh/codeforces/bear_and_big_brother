#include<bits/stdc++.h>
using namespace std;

int main (){
    int a,b,i=0;
    cin>>a>>b;
    int sum=a,sum1=b;
    // cout<<sum<<" "<<sum1<<"\n";
    while (sum <= sum1)
    {
        sum *= 3;
        // cout<<sum<<"\n";
        sum1 *= 2;
        // cout<<sum1<<"\n";
        i++;
        // cout<<i<<"\n";
    }
    cout<<i;
}

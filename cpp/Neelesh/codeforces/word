#include<bits/stdc++.h>
using namespace std;

int main(){
    string s;
    cin>>s;
    int count = 0,count1 = 0;
    for(int i=0;i<s.length();i++){
        if(s[i] >= 65 && s[i] <= 90){
            count++;
        }
        else if(s[i] >= 97 && s[i] <= 122){
            count1++;
        }
    }
    if(count > count1){
        for(int i=0;i<s.length();i++){
            if(s[i] >= 97 && s[i] <= 122){
            s[i] -= 32;
        }
        }
    }
    else if(count <= count1){
        for(int i=0;i<s.length();i++){
            if(s[i] >= 65 && s[i] <= 90){
            s[i] += 32;
        }
        }
    }
    cout<<s;
}

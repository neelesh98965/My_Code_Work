#include<iostream>
using namespace std;

int main()
{
    int n;
    cin>>n;
    int arr[n];
    for(int i=0;i<n;i++){
        cin>>arr[i];
        if(arr[i]<38){
            cout<<arr[i]<<"\n";
        }
        else if((arr[i]+1)%5 == 0)
        {
            cout<<arr[i]+1<<"\n";
        }
        else if((arr[i]+2)%5 == 0)
        {
            cout<<arr[i]+2<<"\n";
        }
        else{
            cout<<arr[i]<<"\n";
        }
    }
    return 0;
}

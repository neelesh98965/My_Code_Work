#include<iostream>
using namespace std;

int main()
{
    ios_base::sync_with_stdio(false);
   cin.tie(NULL);
    int t,k = 1;
    cin>>t;
    while(t--){
        int n;
        cin>>n;
        n = n-5;
        cout<<"Case "<<k<<": ";
        if(n%3 == 0){
            cout<<n<<"\n";
        }
        else{
            cout<<"-1"<<"\n";
        }
        k++;
    }
    return 0;
}

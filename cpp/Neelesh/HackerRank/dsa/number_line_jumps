
#include<iostream>
using namespace std;

int main()
{
    int x1,v1,x2,v2,sum = 0,sum1 = 0;
    cin>>x1>>v1>>x2>>v2;
    for(int i = 0; i<10000;i++){
        sum += x1+(v1*i);
        sum1 += x2+(v2*i);
        if(sum == sum1){
            cout<<"YES";
            exit(0);
        }
    }
    cout<<"NO";
    return 0;
}

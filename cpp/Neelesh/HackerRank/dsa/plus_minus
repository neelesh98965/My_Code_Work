#include<iostream>
#include<iomanip>
using namespace std;

int main()
{
    int n;
    float count = 0, count1 = 0, count2 = 0;
    cin>>n;
    int arr[n];
    for(int i = 0; i<n;i++){
        cin>>arr[i];
        if(arr[i]>0){count++;}
        else if(arr[i]<0){count1++;}
        else if(arr[i] == 0){count2++;}
    }
    cout<<(count/n)<<setprecision(6)<<"\n"<<(count1/n)<<"\n"<<(count2/n);
    return 0;
}

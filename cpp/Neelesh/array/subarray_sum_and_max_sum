#include<bits/stdc++.h>
#include<iostream>
using namespace std;

void printingallsubarrays(int arr[],int n){
    int maxi =0;
    for(int i=0;i<n;i++){
        for(int j = i;j<n;j++){
            int sum = 0;
            cout<<"(";
            for(int k =i;k<=j;k++){
                cout<<arr[k]<<",";
                sum += arr[k];
            }
            cout<<")\n";
            cout<<sum<<"\n";
            maxi = max(sum,maxi);
        }
    }
    cout<<"The maximum sum of the subarray is "<<maxi;
}

int main(){

    int arr[] = {10,20,30,40,50,60};
    int n = sizeof(arr)/sizeof(int);
    printingallsubarrays(arr,n);

}

#include<iostream>
using namespace std;

int main()
{
    ios_base::sync_with_stdio(false);
   cin.tie(NULL);
    string s;
    getline(cin,s);
    for (int i = 0; i < s.length(); i++)
    {
        if((s[i]>=65 && s[i]<=90) || (s[i]>=97 && s[i]<=122)){
            s[i] = s[i];
        }
        else{
            s[i] = '\0';
        }
    }
    cout<<s;
    return 0;
}

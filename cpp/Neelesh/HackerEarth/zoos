#include<iostream>
using namespace std;

int main()
{
    ios_base::sync_with_stdio(false);
   cin.tie(NULL);
    string s;
    int count = 0,count1 = 0;
    cin>>s;
    for (int i = 0; i <s.length(); i++)
    {
        if(s[i] == 'z'){
            count++;
        }
        else if(s[i] == 'o'){
            count1++;
        }
    }
    if(2*count == count1){
        cout<<"Yes";
    }
    else{
        cout<<"No";
    }
    return 0;
}

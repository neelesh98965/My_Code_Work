#include<iostream>
using namespace std;

int main()
{
    ios_base::sync_with_stdio(false);
   cin.tie(NULL);
    int n,sum = 0;
    cin>>n;
    for (int i = 1; i < n; i++)
    {
        if(n%i == 0){
        // cout<<i<<" ";
        sum += i;
        }
    }
    // cout<<sum;
    if(sum == n)
    cout<<"perfect number";
    else
    cout<<"not a perfect number";
    return 0;
}

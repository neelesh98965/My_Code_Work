#include<iostream>
using namespace std;

int main()
{
    ios_base::sync_with_stdio(false);
   cin.tie(NULL);
    char ch;
    cin>>ch;
    if((ch >= 48 && ch <= 57))
    cout<<"Number";
    else if((ch >= 65 && ch <= 90))
    cout<<"Uppercase";
    else if((ch >= 97 && ch <= 122))
    cout<<"Lowercase";
    else
    cout<<"Symbol";
    return 0;
}

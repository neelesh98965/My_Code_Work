#include<iostream>
using namespace std;

class point{
    int a,b;
    public:
    point(int x,int y){
        a=x;
        b=y;
    }
    void displaypoint(){
        cout<<"Your point is ("<<a<<","<<b<<")"<<endl;
    }
};

int main()
{
    point p(4,6);
    p.displaypoint();
    point q(8,9);
    q.displaypoint();
    return 0;
}

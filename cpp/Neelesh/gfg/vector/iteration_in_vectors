#include<bits/stdc++.h>
#include<vector>
using namespace std;

int main(){
    vector<int>g1;
    
    for(int i=1;i<=5;i++)
    g1.push_back(i);

    cout<<"Output of begin and end is : ";
    for(auto i = g1.begin();i != g1.end();++i){
        cout<<*i<<" ";
    }
    
       cout<<"\nOutput of cbegin and cend is : ";
    for(auto i = g1.cbegin();i != g1.cend();++i){
        cout<<*i<<" ";
    }

    cout<<"\nOutput of rbegin and rend is : ";
    for(auto ir = g1.rbegin();ir != g1.rend();++ir){
        cout<<*ir<<" ";
    }

    cout<<"\nOutput of crbegin and crend is : ";
    for(auto ir = g1.crbegin();ir != g1.crend();++ir){
        cout<<*ir<<" ";
    }
}

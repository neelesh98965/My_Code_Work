#include<iostream>
using namespace std;

int main()
{
    ios_base::sync_with_stdio(false);
   cin.tie(NULL);
    string s;
    getline(cin , s);
    for (int i = 0; i < s.length(); i++)
    {
        // cout<<s[i]<<"\n";
        if(s[i] == '0'){
        s[i] = '1';
        // cout<<s[i]<<"\n";
        }
    }
    cout<<s;
    
    return 0;
}

#include<iostream>
using namespace std;

int main()
{
    ios_base::sync_with_stdio(false);
   cin.tie(NULL);
    int n,count = 0;
    cin>>n;
    for (int i = 2; i <= n; i++)
    {
        int a = i;
        while(a>0){
        if(a%10 == 3){
            count++;
            }
             a = a/10;  
        }
    }
    cout<<count;
    return 0;
}
